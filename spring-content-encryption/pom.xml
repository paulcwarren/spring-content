        <version>2.8.0-SNAPSHOT</version>
    </parent>

    <artifactId>spring-content-encryption</artifactId>

    <dependencies>
        <dependency>
            <groupId>org.springframework.data</groupId>
            <artifactId>spring-data-commons</artifactId>
        </dependency>
        <dependency>
